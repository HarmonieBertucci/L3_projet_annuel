<?php
class Comm{
  public $user;
  public $comm;
  public $idAnnonce;
  public $notation;
  function __construct($user, $texte, $idAnnonce , $note){
    $this->user=$user;
    $this->comm=$texte;
    $this->notation=$note;
    $this->idAnnonce=$idAnnonce;
  }


  public function getUser(){
    return $this->user;
  }

  public function getComm(){
    return $this->comm;
  }

  public function getNotation(){
    return $this->notation;
  }

  public function getIdAnnonce(){
    return $this->idAnnonce;
  }
}


 ?>
